{
  "name": "pilas-bloques-backend",
  "version": "1.0.0",
  "description": "Pilas bloques backend",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest --colors --testPathIgnorePatterns dist --testTimeout 5000 --runInBand",
    "start:analytics": "node dist/analytics/index.js",
    "dev:analytics": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/analytics/index.ts",
    "lint": "eslint .",
    "prepackage": "rm -Rf dist && npm run build && cp -R node_modules dist/",
    "package": "tar -czf pilasbloques-backend.tar.gz dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Program-AR/pilas-bloques-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Program-AR/pilas-bloques-backend/issues"
  },
  "homepage": "https://github.com/Program-AR/pilas-bloques-backend#readme",
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/mongodb": "^3.5.26",
    "@types/node": "^14.6.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "release": "^6.3.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@typegoose/typegoose": "^8.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.15"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": false
  }
}

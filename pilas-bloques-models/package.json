  "devDependencies": {
    "@types/mongoose": "5.10.2",
    "@types/node": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "release": "^6.3.1",
    "typescript": "^4.0.2"
  },

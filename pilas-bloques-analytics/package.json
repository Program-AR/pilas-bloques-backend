{
  "name": "pilas-bloques-analytics",
  "version": "1.1.5",
  "description": "REST-API for logging Pilas Bloques usage for scientific purposes.",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "build": "rm -rf dist && tsc",
    "lint": "eslint .",
    "start": "node dist/src/index.js",
    "test": "jest --colors --testPathIgnorePatterns dist --testTimeout 5000 --runInBand",
    "prepackage": "rm -Rf dist && npm run build && cp -R node_modules dist/",
    "package": "tar -czf pilasbloques-analytics.tar.gz dist/*"
  },
  "author": "Fundaci√≥n Sadosky",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^8.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.15",
    "pilas-bloques-models": "file:../pilas-bloques-models"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.10",
    "@types/mongodb": "^3.5.26",
    "@types/node": "^14.6.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "release": "^6.3.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": false
  }
}
